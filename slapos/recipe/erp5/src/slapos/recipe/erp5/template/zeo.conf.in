# ZEO configuration file generated by SlapOS
<zeo>
  address %(zeo_ip)s:%(zeo_port)s
  read-only false
  invalidation-queue-size 100
  pid-filename %(zeo_pid)s
</zeo>

<filestorage %(zeo_storagename)s>
  path %(zeo_zodb)s
</filestorage>

<eventlog>
  <logfile>
    path %(zeo_event_log)s
  </logfile>
</eventlog>

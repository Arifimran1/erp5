CREATE TABLE `message` (
  `uid` int(11) NOT NULL auto_increment,
  `date` datetime,
  `path` VARCHAR(255),
  `method_id` VARCHAR(40),
  `processing_node` INT DEFAULT -1,
  `processing` INT DEFAULT 0,
  `priority` INT DEFAULT 0,
  `message` BLOB,
  PRIMARY KEY  (`uid`),
  KEY `date` (`date`),

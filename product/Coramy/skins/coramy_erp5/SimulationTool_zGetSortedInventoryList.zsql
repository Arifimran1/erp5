SELECT
	catalog.*
FROM
	catalog LEFT JOIN movement ON(catalog.uid=movement.uid)
WHERE
	catalog.portal_type = "Inventory MP"
OR	catalog.portal_type = "Inventory PF"
ORDER BY
	movement.start_date
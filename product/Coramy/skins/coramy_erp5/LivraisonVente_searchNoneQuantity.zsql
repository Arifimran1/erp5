SELECT
	catalog.*
FROM
	catalog LEFT JOIN movement ON (catalog.uid = movement.uid)
WHERE
	catalog.portal_type = "Sales Packing List Line"
AND	movement.is_accountable = 1
AND	movement.has_cell_content = 0
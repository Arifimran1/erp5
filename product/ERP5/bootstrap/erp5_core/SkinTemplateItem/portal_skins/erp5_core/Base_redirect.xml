        <item>
            <key> <string>_body</string> </key>
            <value> <string># You can use this script to jump to another module / document without making the URL longer.\n
# Like \'erp5/organisation/purchase_order/accounting/view\'.\n
#\n
# Usage: .../Base_redirect?relative_url=purchase_order\n
#\n
# If you omit the parameter relative_url, this jumps to the portal (i.e. ERP5 Site).\n
\n
request=context.REQUEST\n
\n
if relative_url is None:\n
  redirect_url = \'%s/view\' % (context.getPortalObject().absolute_url())\n
else:\n
  redirect_url = \'%s/%s/view\' % (context.getPortalObject().absolute_url(), relative_url)\n
\n
request[ \'RESPONSE\' ].redirect( redirect_url )\n
</string> </value>

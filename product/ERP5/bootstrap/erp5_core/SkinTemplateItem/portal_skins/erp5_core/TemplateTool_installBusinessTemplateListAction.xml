        <item>
            <key> <string>_body</string> </key>
            <value> <string>listbox = kw.get(\'listbox\', ())\n
\n
bt_id_list = getattr(context.REQUEST, \'bt_list\', ())\n
bt_dict = {}\n
object_to_update = {}\n
for item in listbox:\n
  bt_id, object_id = item[\'listbox_key\'].split(\'|\')\n
  if bt_dict.has_key(bt_id):\n
    bt_dict[bt_id][object_id] = item[\'choice\']\n
  else:\n
    bt_dict[bt_id] = {}\n
    bt_dict[bt_id][object_id] = item[\'choice\']\n
\n
bt_title_list = []\n
for bt_id in bt_dict.keys():\n
  object_list = bt_dict[bt_id]\n
  bt = context.portal_templates[bt_id]\n
  bt.install(force=0, object_to_update=object_list)\n
  bt_title_list.append(bt.getTitle())\n
\n
for id in bt_id_list:\n
  if id not in bt_dict.keys():\n
    bt = context.portal_templates[id]\n
    bt.install(force=0, object_to_update=[])\n
    bt_title_list.append(bt.getTitle())\n
\n
\n
REQUEST = container.REQUEST\n
RESPONSE = REQUEST.RESPONSE\n
\n
return RESPONSE.redirect("%s?portal_status_message=Business+Template+%s+installed" % (context.absolute_url(), \',+\'.join(bt_title_list)))\n
</string> </value>

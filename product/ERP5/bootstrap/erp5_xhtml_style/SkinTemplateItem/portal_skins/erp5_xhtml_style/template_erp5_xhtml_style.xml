            <key> <string>id</string> </key>
            <value> <string>template_erp5_xhtml_style</string> </value>
        </item>
        <item>
            <key> <string>title</string> </key>
            <value> <string></string> </value>
        </item>
      </dictionary>
    </pickle>
  </record>

                    i18n:domain="ui" i18n:attributes="title" />\n
                </td>\n
                <td style="white-space: nowrap; vertical-align: middle; text-align: center">\n
                  <select id="listbox_page_selection" name="list_start" title="Change Page" size="1"\n

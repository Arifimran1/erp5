var FCKPlaceholders = oEditor.FCKPlaceholders ;\r\n
\r\n
window.onload = function ()\r\n
{\r\n
\t// First of all, translate the dialog box texts\r\n
\toEditor.FCKLanguageManager.TranslatePage( document ) ;\r\n
\r\n
\tLoadSelected() ;\r\n
\r\n
\t// Show the "Ok" button.\r\n
\tdialog.SetOkButton( true ) ;\r\n
\r\n
\t// Select text field on load.\r\n
\tSelectField( \'txtName\' ) ;\r\n
}\r\n
\r\n
var eSelected = dialog.Selection.GetSelectedElement() ;\r\n
\r\n
function LoadSelected()\r\n
{\r\n
\tif ( !eSelected )\r\n
\t\treturn ;\r\n
\r\n
\tif ( eSelected.tagName == \'SPAN\' && eSelected._fckplaceholder )\r\n
\t\tdocument.getElementById(\'txtName\').value = eSelected._fckplaceholder ;\r\n
\telse\r\n
\t\teSelected == null ;\r\n
}\r\n
\r\n
function Ok()\r\n
{\r\n
\tvar sValue = document.getElementById(\'txtName\').value ;\r\n
\r\n
\tif ( eSelected && eSelected._fckplaceholder == sValue )\r\n
\t\treturn true ;\r\n
\r\n
\tif ( sValue.length == 0 )\r\n
\t{\r\n
\t\talert( FCKLang.PlaceholderErrNoName ) ;\r\n
\t\treturn false ;\r\n
\t}\r\n
\r\n
\tif ( FCKPlaceholders.Exist( sValue ) )\r\n
\t{\r\n
\t\talert( FCKLang.PlaceholderErrNameInUse ) ;\r\n
\t\treturn false ;\r\n
\t}\r\n
\r\n
\tFCKPlaceholders.Add( sValue ) ;\r\n
\treturn true ;\r\n
}\r\n

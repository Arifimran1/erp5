\t\t\tvar range = oEditor.FCK.EditorDocument.createRange() ;\r\n
\t\t\trange.selectNode( aCells[i] ) ;\r\n
\t\t\tselection.addRange( range ) ;\r\n
\t\t}\r\n
\t}\r\n

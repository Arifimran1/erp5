<VirtualHost *:${:httpd_port}>
  ServerName       localhost
  DocumentRoot     ${:httpd_htdocs}
  HostnameLookups  Off
  UseCanonicalName Off
  ServerSignature  Off
  RewriteEngine    On
  ProxyVia         on
  ProxyTimeout     1200

${:httpd_vhost_rewrite_rules}

  <Location "/">
    Order Allow,Deny
    Allow from all
  </Location>

</VirtualHost>

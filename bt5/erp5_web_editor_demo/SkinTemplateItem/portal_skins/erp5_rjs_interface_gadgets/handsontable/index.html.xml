          <button name="dump" data-dump="#example" data-instance="hot" title="Prints current data source to Firebug/Chrome Dev Tools">\n
            Dump data to console\n
          </button>\n
        </div>\n
\n
\n
        <script data-jsfiddle="example">\n
          var\n
            data = [\n
              [\'\', \'Maserati\', \'Mazda\', \'Mercedes\', \'Mini\', \'Mitsubishi\'],\n
              [\'2009\', 0, 2941, 4303, 354, 5814],\n
              [\'2010\', 3, 2905, 2867, 412, 5284],\n
              [\'2011\', 4, 2517, 4822, 552, 6127],\n
              [\'2012\', 2, 2422, 5399, 776, 4151]\n
            ],\n
            container = document.getElementById(\'example\'),\n
            hot;\n
\n
          hot = new Handsontable(container, {\n
            data: data,\n
            minSpareRows: 1,\n
            colHeaders: true,\n
            contextMenu: true\n
          });\n
\n
        </script>\n

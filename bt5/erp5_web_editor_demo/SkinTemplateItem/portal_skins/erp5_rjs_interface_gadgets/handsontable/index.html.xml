    <script type="text/javascript">\n
      var showSourceElements = document.querySelectorAll(\'.show-source\');\n
\n
      for(var i = 0; i < showSourceElements.length ; i++) {\n
        var showElem = showSourceElements[i];\n
        showElem.addEventListener(\'mousedown\', function (event) {\n
          event.preventDefault();\n
\n
          if(showElem.className.indexOf(\'shown\') > -1) {\n
            showElem.className = showElem.className.replace(\'shown\', \'\');\n
          } else {\n
            showElem.className += \' shown\';\n
          }\n
\n
          var preJS = document.querySelectorAll(\'pre.javascript\');\n
          for(var j = 0; j < preJS.length; j++) {\n
            if(preJS[j].style.display == \'\') {\n
              preJS[j].style.display = \'block\';\n
            } else {\n
              preJS[j].style.display = \'\';\n
            }\n
          }\n
\n
          var codeLay = document.querySelectorAll(\'.codeLayout\');\n
          for(var j = 0; j < codeLay.length; j++) {\n
            if(codeLay[j].className.indexOf(\'codeLayoutExpanded\') > -1) {\n
              codeLay[j].className = codeLay[j].className.replace(\'codeLayoutExpanded\', \'\');\n
            } else {\n
              codeLay[j].className += \' codeLayoutExpanded\';\n
            }\n
          }\n
\n
        });\n
      }\n
\n
      var verHolders = document.querySelectorAll(\'.ver\');\n
      for(var i = 0; i < verHolders.length; i++) {\n
        var verText = document.createTextNode(hot.version);\n
        verHolders[i].appendChild(verText);\n
      }\n
    </script>\n

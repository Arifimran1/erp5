            var example2 = document.getElementById(\'example2\');\n
            var hot2 = new Handsontable(example2,{\n
              data: getCarData(),\n
              startRows: 7,\n
              startCols: 4,\n
              colHeaders: ["Car", "Year", "Comes in black"],\n
              colWidths: [120, 50, 60],\n
              columnSorting: true,\n
              columns: [\n
                {\n
                  data: "car"\n

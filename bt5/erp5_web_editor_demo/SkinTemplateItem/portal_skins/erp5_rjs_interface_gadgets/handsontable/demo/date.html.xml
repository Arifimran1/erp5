\n
              <script data-jsfiddle="example1">\n
                var container = document.getElementById(\'example1\'),\n
                  hot;\n
\n
                hot = new Handsontable(container, {\n
                  data: getCarData(),\n
                  startRows: 7,\n
                  startCols: 4,\n
                  colHeaders: [\'Car\', \'Model\', \'Registration date\', \'Price\'],\n
                  columnSorting: true,\n
                  contextMenu: true,\n
                  minSpareRows: 1,\n
                  columns: [\n
                    {\n
                      type: \'autocomplete\',\n
                      source: [\'Audi\', \'BMW\', \'Chrysler\', \'Citroen\', \'Mercedes\', \'Nissan\', \'Opel\', \'Suzuki\', \'Toyota\', \'Volvo\'],\n

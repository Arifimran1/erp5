              Handsontable.Dom.addEvent(htOptions[i], \'change\', function () {\n
                var update = {};\n
\n
                switch (this.value) {\n
                  case \'true\':\n
                    update[this.name] = true;\n
                    break;\n
\n
                  case \'false\':\n
                    update[this.name] = false;\n
                    break;\n
\n
                  default:\n
                    update[this.name] = this.value;\n
                }\n

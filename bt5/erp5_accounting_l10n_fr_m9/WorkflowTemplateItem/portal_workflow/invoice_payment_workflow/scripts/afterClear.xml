        <item>
            <key> <string>_body</string> </key>
            <value> <string>"""Called when an Invoice is cleared.\n
"""\n
from AccessControl import getSecurityManager\n
from DateTime import DateTime\n
now = DateTime()\n
invoice = sci[\'object\']\n
portal = invoice.getPortalObject()\n
\n
if invoice.getPortalType() in ( \'Sale Invoice Transaction\',\n
                                \'Balance Transaction\',\n
                                \'Accounting Transaction\' ):\n
  invoice.Invoice_setRecoveryStatus(None)\n
\n
# we only call this script if the invoice has been imported\n
if not invoice.getProperty(\'imported_object\', 0):\n
  return\n
\n
nommaj = str(getSecurityManager().getUser())\n
dateta = now.strftime("%Y%m%d")\n
\n
if invoice.getOriginId() in (\'OR\', \'RV\'):\n
  if invoice.getTitle() is not None:\n
    cleor = invoice.getTitle()\n
    invoice.activate().OracleBridge_onSaleInvoiceClear(\n
                                            IN_NOMMAJ=nommaj,\n
                                            IN_CLEOR=cleor,\n
                                            IN_DATETA=dateta )\n
\n
elif invoice.getOriginId() in (\'MD\',\'CR\', \'PY\', \'MP\', \'RP\',):\n
  if invoice.getIntIndex() is not None and invoice.getTitle() is not None:\n
    nomand = "%05d" % invoice.getIntIndex()\n
    nofact = invoice.getTitle()\n
    invoice.activate().OracleBridge_onPurchaseInvoiceClear(\n
                                                IN_NOMMAJ=nommaj,\n
                                                IN_NOFACT=nofact,\n
                                                IN_DATETA=dateta )\n
</string> </value>

        <item>
            <key> <string>_body</string> </key>
            <value> <string>getEmitYear = context.AccountingTransaction_getEmitYear\n
getEmitDate = context.AccountingTransaction_getEmitDate\n
\n
# TODO: section_category is ignored\n
transaction_list = []\n
total_price = 0\n
for transaction in context.accounting_module.searchFolder(\n
                             simulation_state=simulation_state,\n
                             origin_uid=origin_uid):\n
  transaction = transaction.getObject()\n
  invoice_price = transaction.getTotalPrice()\n
  total_price += invoice_price\n
  transaction_list.append(transaction.asContext(amount=invoice_price,\n
                                                int_index=\'%05d\' % (transaction.getIntIndex() or 0),\n
                                                year=getEmitYear(transaction),\n
                                                emit_date=getEmitDate(transaction)))\n
\n
context.REQUEST.set(\'total_price_%s\' % simulation_state, total_price)\n
return transaction_list\n
</string> </value>

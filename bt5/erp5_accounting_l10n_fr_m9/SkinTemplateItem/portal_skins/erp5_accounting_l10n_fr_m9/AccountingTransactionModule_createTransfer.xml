        <item>
            <key> <string>_body</string> </key>
            <value> <string>from Products.ERP5Type.Message import Message\n
from DateTime import DateTime\n
portal = context.getPortalObject()\n
stool = portal.portal_selections\n
getObject = portal.portal_catalog.getObject\n
invoice_type_list = portal.getPortalInvoiceTypeList()\n
N_ = lambda msg, **kw: Message(\'erp5_ui\', msg, **kw)\n
\n
\n
if payment_transaction_list is None:\n
  # if a list of payment transaction is not passed directly, get it from selection\n
  stool.updateSelectionCheckedUidList(selection_name, listbox_uid, uids)\n
  selection_uid_list = context.portal_selections.getSelectionCheckedUidsFor(\n
                                                       selection_name)\n
  if selection_uid_list:\n
    brain_list = [getObject(uid) for uid in selection_uid_list]\n
  else:\n
    brain_list = stool.callSelectionFor(selection_name)\n
\n
  payment_transaction_list = []\n
  for brain in brain_list:\n
    obj = brain.getObject()\n
    payment_transaction_list.append(obj)\n
    if obj.getPortalType() != \'Payment Transaction\' \\\n
         or obj.getPaymentMode() != \'depenses/virements\' \\\n
         or obj.getSimulationState() != \'delivered\':\n
      return context.REQUEST.RESPONSE.redirect(\n
        "%s/view?portal_status_message=%s" % (context.absolute_url(), N_(\n
        \'Transfer not created. ${transaction_title} does not match.\',\n
     mapping=dict(transaction_title=unicode(obj.getTitle() or \'\', \'utf8\', \'repr\')))))\n
\n
transfer = portal.transfer_module.newContent(\n
              portal_type=\'Transfer\',\n
              start_date=DateTime(), )\n
\n
tag = str(portal.portal_ids.generateNewLongId())\n
activate_kw = dict(tag=tag)\n
for transaction in payment_transaction_list:\n
  transaction.PaymentTransaction_setAggregateValue(transfer, activate_kw=activate_kw)\n
  transaction.emit()\n
\n
# activate a dummy method after all reindexing\n
transfer.activate(after_tag=tag).getTitle()\n
\n
if batch_mode:\n
  return transfer\n
  \n
return context.REQUEST.RESPONSE.redirect(\n
    "%s/view?portal_status_message=%s" % (\n
        transfer.absolute_url(), N_(\'Transfer Created\')))\n
</string> </value>

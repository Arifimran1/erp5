        <item>
            <key> <string>_body</string> </key>
            <value> <string>from DateTime import DateTime\n
portal = context.getPortalObject()\n
N_ = portal.Base_translateString\n
causality = context.getCausalityValue() or context\n
\n
is_source = context.AccountingTransaction_isSourceView()\n
\n
if is_source:\n
  section = context.getSourceSection()\n
  mirror_section = context.getDestinationSection()\n
  qty = sum([l.getSourceInventoriatedTotalAssetCredit() or 0\n
             for l in context.getMovementList() if l.getSourceId() == \'4664\'])\n
else:\n
  section = context.getDestinationSection()\n
  mirror_section = context.getSourceSection()\n
  qty = sum([l.getDestinationInventoriatedTotalAssetCredit() or 0\n
             for l in context.getMovementList() if l.getDestinationId() == \'4664\'])\n
             \n
tr = portal.accounting_module.newContent(\n
                   portal_type=\'Payment Transaction\',\n
                   origin=\'Payment Transaction/OP\',\n
                   created_by_builder=1, # XXX prevent init script\n
                   source_section=section,\n
                   destination_section=mirror_section,\n
                   resource=context.getResource(),\n
                   start_date=DateTime().earliestTime(),\n
                   causality_value=causality )\n
\n
# Add a custom property to make a condition for the action\n
tr.manage_addProperty(\'is_refund_statement\', True, type=\'boolean\')\n
\n
tr.newContent( portal_type=\'Accounting Transaction Line\',\n
\t       source=\'account_module/4664\',\n
\t       source_debit=qty )\n
tr.newContent( portal_type=\'Accounting Transaction Line\',\n
\t       source=\'account_module/5151\',\n
\t       source_credit=qty )\n
\n
portal.portal_workflow.m9_accounting_workflow.scripts.setReferences(\n
          state_change=dict(object=tr))\n
tr.plan()\n
\n
context.REQUEST.RESPONSE.redirect(\'%s/view?portal_status_message=%s\' % (\n
                                  tr.absolute_url(), N_(\'Refund Created.\')))\n
</string> </value>

            <value> <string encoding="cdata"><![CDATA[

from Products.PythonScripts.standard import Object\n
\n
object_list = []\n
movement_urls = {}\n
\n
def getListItemUrl(alias, idx, selection_name):\n
  return movement_urls[idx]\n
\n
idx = -1\n
for movement in movement_list:\n
  idx += 1\n
  if node_relative_url == movement.getSource():\n
    debit = movement.getSourceInventoriatedTotalAssetDebit() or 0\n
    credit = movement.getSourceInventoriatedTotalAssetCredit() or 0\n
    node = movement.getSourceValue()\n
  else:\n
    debit = movement.getDestinationInventoriatedTotalAssetDebit() or 0\n
    credit = movement.getDestinationInventoriatedTotalAssetCredit() or 0\n
    node = movement.getDestinationValue()\n
  int_index = movement.getParentValue().getIntIndex() and (\n
                "%05d" % movement.getParentValue().getIntIndex()) or ""\n
\n
  movement_urls[idx] = movement.getParentValue().absolute_url() + \'/view\'\n
  object_list.append(Object(id=\'%s\' % kw,\n
                         uid=movement.getUid(),\n
                         getListItemUrl=getListItemUrl,\n
                         delivery_title=movement.getParentTitle(),\n
                         delivery_portal_type=movement\\\n
                              .getParentValue().getTranslatedPortalType(),\n
                         delivery_int_index=int_index,\n
                         grouping_reference=movement.getGroupingReference(),\n
                         debit=debit, credit=credit, net=abs(debit-credit),\n
                         net_side=(debit > credit and \'D\' or \'C\')))\n
\n
if stat:\n
  precision = context.getQuantityPrecisionFromResource(context.getResource())\n
  debit = round(sum([x.debit for x in object_list]), precision) or 0\n
  credit = round(sum([x.credit for x in object_list]), precision) or 0\n
  net = abs(round(sum([(x.debit - x.credit)\n
                  for x in object_list]), precision)) or 0\n
  net_side = \'\'\n
  if debit > credit:\n
    net_side = \'D\'\n
  elif credit > debit:\n
    net_side = \'C\'\n
  return [Object( debit=debit, credit=credit, net=net, net_side=net_side)]\n
return object_list\n


]]></string> </value>

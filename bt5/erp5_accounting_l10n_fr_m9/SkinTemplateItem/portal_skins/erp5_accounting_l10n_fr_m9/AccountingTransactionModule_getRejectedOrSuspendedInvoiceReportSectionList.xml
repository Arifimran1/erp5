        <item>
            <key> <string>_body</string> </key>
            <value> <string>from Products.ERP5Form.Report import ReportSection\n
request = context.REQUEST\n
N_ = context.Base_translateString\n
\n
section_category = request.get(\'section_category\')\n
origin = request.get(\'origin\')\n
origin_cat = context.getPortalObject().portal_categories.origin\n
if origin == \'Depenses\':\n
  origin_list = [origin_cat.restrictedTraverse(url) for url in (\n
                     \'Purchase Invoice Transaction\',\n
                     \'Accounting Transaction/RC\'  ) ]\n
else:\n
  assert origin == \'Recettes\'\n
  origin_list = [origin_cat.restrictedTraverse(url) for url in (\n
                     \'Sale Invoice Transaction\',\n
                     \'Accounting Transaction/RV\'  ) ]\n
\n
origin_uid = [x.getUid() for x in origin_list]\n
\n
return [ReportSection(\n
          title=N_(\'Suspended Invoices\'),\n
          path=context.getPhysicalPath(),\n
          form_id=\'AccountingTransactionModule_viewRejectedOrSuspendedInvoiceReportSection\',\n
          selection_params=dict(simulation_state=\'suspended\',\n
                                origin_uid=origin_uid,\n
                                section_category=section_category)),\n
        ReportSection(\n
          title=N_(\'Rejected Invoices\'),\n
          path=context.getPhysicalPath(),\n
          form_id=\'AccountingTransactionModule_viewRejectedOrSuspendedInvoiceReportSection\',\n
          selection_params=dict(simulation_state=\'rejected\',\n
                                origin_uid=origin_uid,\n
                                section_category=section_category))]\n
</string> </value>

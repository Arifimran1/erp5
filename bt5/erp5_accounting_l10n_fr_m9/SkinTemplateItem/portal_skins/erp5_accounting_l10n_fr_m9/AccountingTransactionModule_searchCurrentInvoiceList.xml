        <item>
            <key> <string>_body</string> </key>
            <value> <string encoding="cdata"><![CDATA[

from DateTime import DateTime\n
from ZTUtils import make_query\n
\n
N_ = context.Base_translateString\n
portal = context.getPortalObject()\n
\n
def getOriginUid(url):\n
  return portal.portal_categories.origin.restrictedTraverse(url).getUid()\n
\n
now = DateTime()\n
payment_state = \'current\'\n
if show_suspended_transaction:\n
  payment_state = [\'current\', \'suspended\']\n
search_kw = dict( reset=1,\n
                  simulation_state=\'delivered\',\n
                  portal_type=[\'Sale Invoice Transaction\',\n
                               \'Accounting Transaction\',\n
                               \'Balance Transaction\'],\n
                  origin_uid=[getOriginUid(o) for o in (\'Accounting Transaction/RV\',\n
                                                       \'Sale Invoice Transaction/OR\')],\n
                  payment_state=payment_state )\n
\n
# update dialog selection\n
dialog_selection_name = "accounting_current_invoice_selection"\n
portal.portal_selections.setSelectionParamsFor(dialog_selection_name,\n
          params=dict( payment_day_late_range_max=payment_day_late_range_max,\n
                       payment_day_late_range_min=payment_day_late_range_min,\n
                       recovery_status=recovery_status,\n
                       third_party_activity=third_party_activity,\n
                       show_suspended_transaction=show_suspended_transaction ))\n
\n
if payment_day_late_range_min != \'\' and \\\n
          payment_day_late_range_max < payment_day_late_range_min:\n
  return context.REQUEST.RESPONSE.redirect(\n
       \'%s/AccountingTransactionModule_viewSearchCurrentInvoiceDialog\'\\\n
           \'?cancel_url=%s&dialog_category=%s&selection_name=%s&\'\\\n
           \'portal_status_message=%s\' % (\n
       context.absolute_url(), cancel_url, dialog_category, selection_name,\n
       N_(\'Min+is+greater+than+Max.\')))\n
\n
if payment_day_late_range_min:\n
  search_kw[\'at_date\'] = now - payment_day_late_range_min\n
if payment_day_late_range_max:\n
  search_kw[\'from_date\'] = now - payment_day_late_range_max\n
if third_party_activity:\n
  search_kw[\'delivery_mirror_section_activity_uid\'] = \\\n
          portal.portal_categories.activity.restrictedTraverse(\n
              third_party_activity).getUid()\n
if recovery_status:\n
  search_kw[\'recovery_status_relative_url\'] = recovery_status\n
\n
return context.REQUEST.RESPONSE.redirect(\n
    \'%s/list?%s\' % (context.absolute_url(), make_query(search_kw)))\n


]]></string> </value>

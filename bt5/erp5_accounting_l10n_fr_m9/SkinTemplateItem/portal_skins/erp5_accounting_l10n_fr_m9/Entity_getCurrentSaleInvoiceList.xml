        <item>
            <key> <string>_body</string> </key>
            <value> <string>request = container.REQUEST\n
\n
origin_cat = context.getPortalObject().portal_categories.origin\n
transaction_list = []\n
total_amount = 0\n
for transaction in context.portal_catalog(\n
            origin_uid=[origin_cat.restrictedTraverse(url).getUid()\n
                       for url in (\'Accounting Transaction/RV\',\n
                                   \'Sale Invoice Transaction/OR\')],\n
            simulation_state=\'delivered\',\n
            payment_state=[\'current\', \'suspended\'],\n
            **{\'delivery.destination_section_uid\': context.getUid()}):\n
  transaction = transaction.getObject()\n
  amount = transaction.Invoice_getRemainingTotalPayablePrice()\n
  total_amount += amount\n
  transaction_list.append(transaction.asContext(amount=amount))\n
\n
request.set(\'%s/Entity_statCurrentSaleInvoiceList\' % context.getUid(),\n
            total_amount)\n
return transaction_list\n
</string> </value>

        <item>
            <key> <string>_body</string> </key>
            <value> <string>from Products.DCWorkflow.DCWorkflow import ValidationFailed\n
\n
my_transaction = state_change.object\n
\n
etat = context.getSimulationState()\n
transition = state_change.transition.id\n
method = getattr(context, transition) \n
\n
error_message=\'\'\n
\n
if transition == \'reject\' and etat == \'planned\' : \n
  error_message=\'Op\xc3\xa9ration renvoy\xc3\xa9e en erreur !\'\n
elif etat == \'planned\' :\n
  error_message=\'Signature conforme !\'\n
elif etat == \'ordered\' :\n
  error_message=\'Op\xc3\xa9ration en validation manuelle pour provision insuffisante !\'\n
elif etat == \'confirmed\' :\n
  error_message=\'Op\xc3\xa9ration envoy\xc3\xa9e \xc3\xa0 la caisse pour paiement !\'\n
elif etat == \'delivered\' :\n
  error_message=\'Paiement effectu\xc3\xa9e ! Op\xc3\xa9ration termin\xc3\xa9e.\'\n
\n
\n
state_change.setWorkflowVariable(state_change.object,\n
  workflow_id=\'check_payment_workflow\',\n
  error_message=error_message)\n
</string> </value>

        <item>
            <key> <string>_body</string> </key>
            <value> <string encoding="cdata"><![CDATA[

from Products.DCWorkflow.DCWorkflow import ValidationFailed\n
\n
transaction = state_change.object\n
\n
caisse = transaction.getSource()\n
resource = context.Caisse_checkInventory(source= caisse,portal_type=\'Check Payment Line\')\n
\n
# Get price and total_price.\n
price = transaction.getPrice()\n
billetage = transaction.getTotalPrice(deliveryLineType=\'Check Payment Line\')\n
\n
if resource == 3 :\n
  raise ValidationFailed, \'Aucun billetage saisi !!! Veuillez v\xc3\xa9rifier\' \n
elif price != billetage :\n
  raise ValidationFailed, \'Le montant saisi est diff\xc3\xa9rent du billetage\'\n
elif resource == 2 :\n
  raise ValidationFailed, \'Aucune ressource !!! Veuillez v\xc3\xa9rifier\'\n
elif resource == 1 :\n
  raise ValidationFailed, \'Insufficient balance\'\n
\n
# elif len(transaction.checkConsistency()) > 7 :\n
#  raise ValidationFailed, \'Erreur dans les donn\xc3\xa9es! Veuillez v\xc3\xa9rifier\'\n


]]></string> </value>

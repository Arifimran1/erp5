        <item>
            <key> <string>_body</string> </key>
            <value> <string encoding="cdata"><![CDATA[

if not context.VariationList_ok(listbox):\n
  context.Base_updateDialogForm(listbox=listbox, empty_line_number=0)\n
  return context.asContext(context=None,portal_type=context.getPortalType(), **kw).Container_fastInputForm(**kw)\n
\n
\n
# list of all variation in the portal category\n
catalog_variation_list =  context.portal_categories.variation.getCategoryChildTitleItemList()[1:]   \n
\n
base_list=[\'variation\']\n
counter_line=0\n
\n
containerLines_toDelete = []\n
# List of elements on the input listbox\n
for x in listbox :    # Element of ListBox\n
  counter = 1\n
  variation_list = [] \n
  quantity_dictionary = {}                     # Les off cellule with value \n
  for y in catalog_variation_list:            # Element of variation (Millesime)\n
    cell_quantity = int(x[\'variation\'+str(counter)] or 0)\n
    if cell_quantity <> 0 :                              # input quantity in the listbox\n
       variation_list.append(\'variation/\'+y[1])          # Preparation of variation in the matrix for 1 check payment line \n
       quantity_dictionary[\'variation/\'+y[1]] = cell_quantity\n
    counter  += 1\n
\n
  old_line = context.searchContainerLineByResource(x[\'Id_resource\'])   # Search if line exist on ContainerLine\n
 \n
  if len(variation_list) > 0 :          # We have quantity without zero value\n
    counter_line += 1\n
    if old_line is None:                # No Line found, we have to create it\n
       new_line = context.newContent(portal_type=\'Container Line\')             # New line creation\n
       resource_list = context.portal_catalog(portal_type = (\'Banknote\',\'Coin\'),id = x[\'Id_resource\'])   # Resource search\n
       resource = resource_list[0].getObject()      # Found object with the Catalog object Id \n
       new_line.setResourceValue(resource)          # Set line resource by object found\n
    else :\n
       new_line = old_line\n
    \n
    resource_list = context.portal_catalog(portal_type = (\'Banknote\',\'Coin\'),id = x[\'Id_resource\']) \n
    resourceVariation_list = resource_list[0].getObject().getVariationIdList()       # List of possible variation on th resource\n
 \n
    resultVariationDiff = [\'variation/\'+ x for x in resourceVariation_list  if (\'variation/\'+x) not in variation_list ]\n
    if not resultVariationDiff is None : \n
      variation_list = variation_list + resultVariationDiff \n
      for x in resultVariationDiff :  \n
        quantity_dictionary[x] = 0\n
\n
    new_line.setVariationBaseCategoryList(base_list)\n
    new_line.setVariationCategoryList(variation_list)\n
    for z in variation_list :\n
        cell = new_line.getCell(z)\n
        cell.setQuantity(quantity_dictionary[z])\n
  else:\n
    if not old_line is None:\n
       containerLines_toDelete.append(old_line.getId())\n
\n
if len(containerLines_toDelete) <> 0 :\n
   context.manage_delObjects(containerLines_toDelete)\n
    \n
 \n
request  = context.REQUEST\n
redirect_url = \'%s/view?%s\' % ( context.absolute_url()\n
                                , \'portal_status_message=\'+str(counter_line)+ \' + ligne(s)+cr\xc3\xa9\xc3\xa9e(s)...\'\n
                                )\n
request[ \'RESPONSE\' ].redirect( redirect_url )\n


]]></string> </value>

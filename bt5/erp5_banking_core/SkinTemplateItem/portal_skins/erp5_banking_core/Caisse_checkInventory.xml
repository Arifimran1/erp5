            <key> <string>_body</string> </key>
            <value> <string encoding="cdata"><![CDATA[

# check that the amount of resources at `source` is greater or equal\n
# than the amount of movement of type `portal_type` in this movement\n
\n
# returns {\'error_code\', \'resource1\':quantity1, \'resource2\':quantity2, ...}\n
\n
currency_cash_portal_type_list = [\'Banknote\', \'Coin\']\n
\n
inventory_dict = {}\n
requested_dict = {}\n
\n
# test all movement\n
tmp_list = context.contentValues(filter={\'portal_type\': portal_type})\n
line_list = []\n
for l in tmp_list :\n
  # The source can be different for every line (due to getBaobabSource approach)\n
  source_caisse = source or l.getBaobabSource()\n
  # XXX Use of try is not a good idea - it hides errors\n
  try :\n
    # test if resource is a currency_cash\n
    if (l.getResourceValue().getPortalType() in currency_cash_portal_type_list) \\\n
      and (l.getBaobabSource() == source_caisse) :\n
      line_list.append(l)\n
  except :\n
    pass\n
\n
if len(line_list) == 0 :\n
  return 2\n
\n
for line in line_list :\n
  line_resource = line.getResource()\n
  # The source can be different for every line (due to getBaobabSource approach)\n
  source_caisse = source or line.getBaobabSource()\n
  if line.hasCellContent() :\n
    for cell in line.objectValues() :\n
      variation_text = cell.getVariationText()\n
\n
      inventory_value = context.portal_simulation.getFutureInventory(node=source_caisse, resource = line_resource,\n
          variation_text = variation_text)\n
      if debug == 0 and inventory_value - cell.getQuantity() < 0 :\n
        return 1\n
      if debug == 1 :\n
        print \'Asking for %s of %s (%s) :\\n there are %s available at %s\\n\' % (cell.getQuantity(), line.getResourceTitle(),\n
           repr(variation_text), inventory_value, source_caisse)\n
\n
  else :\n
    inventory_value = context.portal_simulation.getFutureInventory(section=source_caisse, resource=line.getResource())\n
\n
    if debug == 0 and inventory_value - line.getQuantity() < 0 :\n
      return 1\n
    if debug == 1 :\n
      print \'Asking for %s of %s\\n : there are %s available at %s\\n\' % (line.getQuantity(), line.getResourceTitle(),\n
          inventory_value, source_caisse)\n
\n
if debug == 1 :\n
  print \'finished\'\n
  return printed\n
\n
return 0\n


]]></string> </value>

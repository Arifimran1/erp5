            <value> <string>def sort(a, b):\n
  return cmp(a[0], b[0])\n
\n
item_portal_type = \'Use Case Actor\'\n
\n
item_list = [[\'\', \'\']]\n
\n
if item_portal_type == None:\n
  return item_list \n
\n
context_obj = context.getObject()\n
\n
obj_list = context_obj.contentValues(filter={\'portal_type\': item_portal_type})\n
for obj in obj_list:\n
  url = obj.getRelativeUrl()\n
  label = obj.getTitle()\n
  item_list.append([label, url])\n
\n
item_list.sort(sort)\n
return item_list\n

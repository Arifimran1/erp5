            <value> <string encoding="cdata"><![CDATA[

item_list = [\'<?xml version="1.0" encoding="UTF-8"?>\',\n
             \'<urlset xmlns="http://www.sitemaps.org/schemas/sitemap/0.9">\',\n
            ]\n
\n
web_site = context.getWebSiteValue()\n
\n
# Sections\n
for section in web_site.WebSection_getSiteMapTree(depth=2):\n
  item_list.extend([\'<url>\',\n
                    \'<loc>%s</loc>\' % section[\'url\'],\n
                    \'</url>\',\n
                   ])   \n
  subsection_list = section[\'subsection\']\n
  if subsection_list:\n
    for subsection in subsection_list:\n
      item_list.extend([\'<url>\',\n
                        \'<loc>%s</loc>\' % subsection[\'url\'],\n
                        \'</url>\',\n
                       ])   \n
\n
item_list.append(\'</urlset>\')\n
return \'\\n\'.join(item_list)\n



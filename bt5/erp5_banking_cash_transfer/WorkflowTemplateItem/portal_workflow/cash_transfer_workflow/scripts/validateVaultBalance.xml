        <item>
            <key> <string>_body</string> </key>
            <value> <string encoding="cdata"><![CDATA[

from Products.DCWorkflow.DCWorkflow import ValidationFailed\n
\n
transaction = state_change.object\n
\n
vault = transaction.getSource()\n
resource = transaction.Caisse_checkInventory(source=vault, portal_type=\'Cash Transfer Line\')\n
\n
\n
# Get price and total_price.\n
amount = transaction.getPrice()\n
total_price = transaction.getTotalPrice(deliveryLineType=\'Cash Transfer Line\')\n
\n
if resource == 2:\n
  raise ValidationFailed, \'No resource.\'\n
elif amount != total_price:\n
  raise ValidationFailed, \'Amount differ from total price.\'\n
elif resource <> 0 :\n
  raise ValidationFailed, \'Insufficient balance.\'\n


]]></string> </value>

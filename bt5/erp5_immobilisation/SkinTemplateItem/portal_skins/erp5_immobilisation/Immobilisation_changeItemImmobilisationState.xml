        <item>
            <key> <string>_body</string> </key>
            <value> <string>request = context.REQUEST\n
error_message = None\n
\n
if context.isCurrentlyImmobilised():\n
  # Unimmobilisation\n
  context.unimmobilise()\n
  error_message = \'Item is unimmobilised\'\n
  \n
else:\n
  # Immobilisation\n
  context.immobilise()\n
  error_message = "Item is immobilised"\n
\n
redirect_url = \'%s/%s?%s%s\' % ( context.absolute_url(),\n
                                form_id,\n
                                \'portal_status_message=\',\n
                                error_message )\n
\n
request[ \'RESPONSE\' ].redirect( redirect_url )\n
</string> </value>

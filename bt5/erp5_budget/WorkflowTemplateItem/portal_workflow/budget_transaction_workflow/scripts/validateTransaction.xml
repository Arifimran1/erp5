            <value> <string encoding="cdata"><![CDATA[

from Products.DCWorkflow.DCWorkflow import ValidationFailed\n
error_message = \'\'\n
\n
# Budget Transaction.\n
transaction = state_change.object\n
\n
# Get source section,destination and the quantity we want to move.\n
source_cell = transaction.getSourceValue()\n
destination_cell = transaction.getDestinationValue()\n
quantity = transaction.getQuantity()\n
balance = source_cell.getCurrentBalance()\n
\n
if source_cell in (None, \'\'):\n
  error_message = \'source is not defined\'\n
if destination_cell in (None, \'\'):\n
  error_message = \'destination is not defined\'\n
\n
if error_message != \'\':\n
  raise ValidationFailed, \'Action impossible : \'+error_message\n
\n
\n
if balance <= quantity:\n
   #raise \'mayoro\', repr((balance,quantity)) \n
   raise ValidationFailed, \'Action impossible : the quantity of the transaction is greater than the balance of the source: balance = %.2f\' % balance\n
\n
budget_list = context.getPortalObject().budget_module.objectValues()\n
max_quantity = 0\n
for obj in budget_list:\n
  for item in obj.objectValues(): \n
     if item.getPortalType() == \'Budget Transfert Line\' and item.getSourceValue() == source_cell and item.getDestinationValue() == destination_cell:\n
         max_quantity = item.getQuantity()\n
if quantity > max_quantity:\n
   raise ValidationFailed, \'Action impossible : the quantity of the transaction is greater than the transferable maximum quantity (TMQ): TMQ = %.2f\' % max_quantity\n


]]></string> </value>

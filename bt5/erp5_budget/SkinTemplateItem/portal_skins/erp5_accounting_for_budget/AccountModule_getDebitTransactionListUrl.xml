        <item>
            <key> <string>_body</string> </key>
            <value> <string>from ZTUtils import make_query\n
\n
index = selection.getIndex()\n
name = selection.getName()\n
object = brain.getObject()\n
try :   url = object.absolute_url()\n
except: url = ""\n
method = \'Account_viewAccountingTransactionList\'\n
kw = { \'selection_index\': str(index),\n
       \'selection_name\' : name, \n
       \'reset\' : \'1\', \n
      # omit_output is disabled because it stays in the selection\n
      #\'omit_output\' : \'1\',\n
     }\n
\n
params = selection.getParams()\n
\n
try:\n
  path = params.get(\'accounting_transaction_line_currency\')\n
  if path:\n
    currency = context.restrictedTraverse(path)\n
  else:\n
    currency = None\n
except:\n
  currency = None\n
\n
if params.get(\'from_date\'):\n
  kw[\'from_date\'] = params[\'from_date\']\n
if params.get(\'to_date\'):\n
  kw[\'to_date\'] = params[\'to_date\']\n
if currency is not None:\n
  kw[\'resource_uid\'] = [currency.getUid()]\n
if params.get(\'transaction_simulation_state\'):\n
  kw[\'transaction_simulation_state\'] = params[\'transaction_simulation_state\']\n
if params.get(\'transaction_section_category\'):\n
  kw[\'transaction_section_category\'] = params[\'transaction_section_category\']\n
\n
return url + \'/\' + method + \'?\' + make_query(kw)\n
</string> </value>

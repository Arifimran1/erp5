        <item>
            <key> <string>_body</string> </key>
            <value> <string>kw.update(selection.getParams())\n
params = {}\n
\n
# read settings from accounting selection\n
# prepare kw parameters\n
accounting_selection_params = context.portal_selections.getSelectionParams("accounting_selection")\n
if accounting_selection_params.get(\'from_date\') : \n
  params[\'from_date\'] = accounting_selection_params[\'from_date\']\n
if accounting_selection_params.get(\'to_date\') : \n
  params[\'to_date\'] = accounting_selection_params[\'to_date\']\n
if accounting_selection_params.get(\'simulation_state\') : \n
  params[\'simulation_state\'] = accounting_selection_params[\'simulation_state\']\n
if accounting_selection_params.get(\'section_category\') : \n
  params[\'section_category\'] = accounting_selection_params[\'section_category\']\n
else : \n
  params[\'section_category\'] = context.getPortalDefaultSectionCategory()\n
try:\n
  url = kw[\'accounting_transaction_line_currency\']\n
  if url:\n
    currency = context.restrictedTraverse(url)\n
    params[\'resource_uid\'] = [currency.getUid()]\n
except KeyError:\n
  pass\n
\n
# build the query with portal_catalog\n
query = context.portal_catalog.buildSQLQuery(**kw)\n
\n
if query.get(\'where_expression\'):\n
  params[\'where_expression\'] = query[\'where_expression\']\n
if query.get(\'from_table_list\'):\n
  params[\'from_table_list\'] = query[\'from_table_list\']\n
\n
if selection.getDomain() :\n
  params[\'selection_domain\'] = selection.getDomain()\n
if selection.getReport() :\n
  params[\'selection_report\'] = selection.getReport()\n
if kw.get(\'closed_summary\'):\n
  params[\'closed_summary\'] = kw[\'closed_summary\']\n
if selection.isInvertMode() : \n
  params[\'selection_uids\'] = selection.getInvertModeUidList()\n
\n
if kw.get(\'omit_input\'):\n
  params[\'omit_input\'] = 1\n
if kw.get(\'omit_output\'):\n
  params[\'omit_output\'] = 1\n
\n
results = context.Resource_zStatInventory(omit_simulation=1,\n
                                         **params)\n
row = results[0]\n
total = 0\n
if row.quantity :\n
  total = row.quantity or 0.00\n
\n
return total\n
</string> </value>

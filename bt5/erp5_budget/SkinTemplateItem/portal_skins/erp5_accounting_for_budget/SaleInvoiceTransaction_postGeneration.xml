        <item>
            <key> <string>_body</string> </key>
            <value> <string>#\n
#  this script is called on the Sale Invoice Transaction\n
# after the delivery builder has finished his job\n
#\n
\n
LOG = lambda msg:context.log("SaleInvoiceTransaction_postDeliveryBuilding", msg)\n
sale_invoice = context\n
\n
# XXX Call the causality workflow ??????\n
sale_invoice.activate().edit()\n
\n
# Accounting specific: \n
#  if every lines have the same resource, then copy the resource on the Transaction.\n
#  XXX and delete resource on the line ? \n
resources_keys = {}\n
for line in sale_invoice.objectValues() :\n
  if line.getPortalType() != "Sale Invoice Transaction Line" : continue\n
  resources_keys[line.getResource()] = 1\n
LOG("resources = %s"%resources_keys)\n
\n
if len(resources_keys.keys()) == 1 :\n
  sale_invoice.setResource(resources_keys.keys()[0])\n
</string> </value>

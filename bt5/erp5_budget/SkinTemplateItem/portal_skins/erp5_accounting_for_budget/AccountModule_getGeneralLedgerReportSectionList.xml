            <key> <string>_body</string> </key>
            <value> <string encoding="cdata"><![CDATA[

"""\n
Get the report sections for general ledger\n
"""\n
from Products.ERP5Form.Report import ReportSection\n
request = context.REQUEST\n
\n
to_date = request[\'to_date\']\n
transaction_section_category = request[\'transaction_section_category\']\n
transaction_simulation_state = request[\'transaction_simulation_state\']\n
from_date = request.get(\'from_date\', None)\n
gap = request.get(\'gap\', None)\n
omit_zero_transactions = request.get(\'omit_zero_transactions\', 0)\n
\n
params =  { \n
            \'to_date\'   : to_date,\n
            \'section_category\' : transaction_section_category,\n
            \'transaction_section_category\' : transaction_section_category,\n
            \'simulation_state\' :  transaction_simulation_state,\n
            \'accounting_transaction_line_currency\' : None,\n
          }\n
# XXX we must use accounting_selection, because stats script reads explicitly accouting_selection\n
selection_name = "accounting_selection"\n
\n
\n
if from_date: \n
    params[\'from_date\'] = from_date\n
if omit_zero_transactions : \n
  ### XXX this requires to add stock table in z_count_result, or use a custom \n
  ### count method in Account_viewAccountingTransactionList/listbox\n
  params[\'where_expression\'] = \'stock.quantity <> 0\'\n
\n
result = []\n
portal = context.portal_url.getPortalObject()\n
\n
account_columns = (    \n
                       (\'source_reference\', \'Reference\'), \n
                       (\'delivery.stop_date\', \'Date\'),\n
                       (\'title\', \'Title\'),\n
                       (\'Type\', \'Type\'),\n
                       (\'getDefaultDestinationSectionTitle\', \'Third Party\'),\n
                       (\'simulation_state\', \'State\'),\n
                       (\'source_debit\', \'Debit\'),\n
                       (\'source_credit\', \'Credit\'),\n
                       (\'source_balance\', \'Balance\'),\n
                     )\n
\n
if gap : \n
  gap_root = [ portal.portal_categories.gap.restrictedTraverse(gap) ] + \\\n
               portal.portal_categories.gap.restrictedTraverse(gap).Category_getSortedCategoryChildValueList() \n
else : \n
  cat = context.portal_selections.getSelectionParams(\'account_selection\').get("default_gap_root") or context.getPortalDefaultGapRoot()\n
  gap_root = portal.portal_categories.resolveCategory(cat).Category_getSortedCategoryChildValueList()\n
\n
for c in gap_root : \n
  account_list = c.getGapRelatedValueList(portal_type="Account")\n
  strict_account_list = c.getGapRelatedValueList(portal_type="Account", strict_membership = 1)\n
  \n
  if len(account_list) and len(strict_account_list) != 1 :\n
    result.append(\n
                 ReportSection(path=c.getPhysicalPath(), \n
                               title="%s: %s" % (c.getId(), c.getTitle()), \n
                               level=len(c.getPhysicalPath()) - 4,\n
                               form_id=None) )\n
\n
  # TODO: detail for account_type/cash account_type/receivable account_type/payable, as in trial balance\n
  if len(strict_account_list):\n
    for account in strict_account_list:\n
      result.append(\n
                 ReportSection(path=account.getPhysicalPath(), \n
                               title="%s: %s" % (c.getId(), account.getTitle()),\n
                               level=9,\n
                               form_id=\'Account_viewAccountingTransactionList\',\n
                               selection_name=selection_name,\n
                               selection_params=params,\n
                               selection_columns=account_columns,\n
                               listbox_display_mode=\'FlatListMode\',\n
                               selection_sort_order=[(\'delivery.stop_date\', \'ascending\')],\n
                              )\n
               )\n
\n
return result\n


]]></string> </value>

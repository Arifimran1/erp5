        <item>
            <key> <string>_body</string> </key>
            <value> <string># This script returns the gap id for this account according to the current gap\n
\n
# TODO : what is the *current gap* ????\n
current_gap = context.getPortalDefaultGapId()\n
\n
for gap in context.getGapValueList() : \n
  if current_gap in gap.getPath() : \n
     return gap.getId()\n
\n
\n
  gap_root = context.getPortalDefaultGapRoot()\n
obj = context.portal_categories.resolveCategory(gap_root)\n
item_list = obj.getCategoryChildItemList(base=0, display_method=display, sort_method=sort)\n
return item_list\n
</string> </value>

        <item>
            <key> <string>_body</string> </key>
            <value> <string>params = selection.getParams()\n
kwd = {}\n
\n
# read settings from accounting selection\n
accounting_selection_params = context.portal_selections.getSelectionParams("accounting_selection")\n
if accounting_selection_params.get(\'from_date\') : \n
  kwd[\'from_date\'] = accounting_selection_params[\'from_date\']\n
if accounting_selection_params.get(\'to_date\') : \n
  kwd[\'to_date\'] = accounting_selection_params[\'to_date\']\n
if accounting_selection_params.get(\'simulation_state\') : \n
  kwd[\'simulation_state\'] = accounting_selection_params[\'simulation_state\']\n
if accounting_selection_params.get(\'section_category\') : \n
  kwd[\'section_category\'] = accounting_selection_params[\'section_category\']\n
else : \n
  kwd[\'section_category\'] = context.getPortalDefaultSectionCategory()\n
query = context.portal_catalog.buildSQLQuery(**kwd)\n
\n
if query.get(\'where_expression\'):\n
  kwd[\'where_expression\'] = query[\'where_expression\']\n
if query.get(\'from_table_list\'):\n
  kwd[\'from_table_list\'] = query[\'from_table_list\']\n
\n
resource = params.get(\'accounting_transaction_line_currency\')\n
if resource:\n
  currency = context.portal_categories.resolveCategory(resource)\n
  kwd[\'resource_uid\'] = [currency.getUid()]\n
\n
if kw.get(\'stat\') : \n
  if selection.getDomain() :\n
    kwd[\'selection_domain\'] = selection.getDomain()\n
  if selection.getReport() :\n
    kwd[\'selection_report\'] = selection.getReport()\n
  if kw.get(\'closed_summary\'):\n
    kwd[\'closed_summary\'] = kw[\'closed_summary\']\n
  if selection.isInvertMode() : \n
    kwd[\'selection_uids\'] = selection.getInvertModeUidList()\n
  result = context.Resource_zStatInventory(omit_simulation=1,\n
                                         **kwd)\n
  row = result[0]\n
  return int(row.count or 0)\n
\n
inventory = context.Resource_zGetInventory_old(node_uid=context.getUid(),\n
                                               omit_simulation=1,\n
                                               **kwd) \n
inventory = inventory[0]\n
return inventory.stock_uid\n
\n
# XXX should be return currency.getInventoryCount(from_date = params.get(\'from_date\'), to_date=params.get(\'to_date\'),  )\n
# XXX should be actually return currency.getInventoryCount(**params  )\n
</string> </value>

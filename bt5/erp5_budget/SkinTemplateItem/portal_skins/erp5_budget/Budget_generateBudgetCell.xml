        <item>
            <key> <string>_body</string> </key>
            <value> <string>context_obj = context.getObject()\n
\n
budget_line_type    = \'Budget Line\'\n
budget_cell_type    = \'Budget Cell\'\n
\n
\n
if context_obj.getPortalType() == budget_line_type:\n
  # we are in a budget line, so create budget cells\n
  destination_obj = context_obj\n
else:\n
  return context.REQUEST.RESPONSE.redirect(context.absolute_url() + \'?portal_status_message=Error:+bad+context.\')\n
\n
\n
# this list contain all budget_cell items\n
budget_cell_items = []\n
\n
# get the user information\n
for budget_cell_line in listbox:\n
  if budget_cell_line.has_key(\'listbox_key\'):\n
    budget_cell_line_id = int(budget_cell_line[\'listbox_key\'])\n
    budget_cell = {}\n
    budget_cell[\'id\'] = budget_cell_line_id\n
    budget_cell[\'max_quantity\'] = budget_cell_line[\'max_quantity\']\n
    budget_cell_items.append(budget_cell)\n
\n
# sort the budget_cells list by id to have the same order of the user\n
budget_cell_items.sort(lambda x, y: cmp(x[\'id\'], y[\'id\']))\n
\n
\n
clean_budget_cells = {}\n
id = 0\n
# scan every fast input line to create budget cells\n
for budget_cell_item in budget_cell_items:\n
  # the item has a value of max_quantity\n
  if budget_cell_item[\'max_quantity\'] not in (\'\', None): \n
       id += 1\n
       clean_budget_cells[id] = budget_cell_item\n
\n
\n
\n
# create budget cells objects  \n
#for key in clean_budget_cells.keys():\n
#    for item in clean_budget_cells[key]:\n
#          my_id = \'movment_\' + str(key) \n
#          new_budget_cell = destination_obj.newContent( portal_type  = budget_cell_type\n
#                                                      , id           = my_id\n
#                                                      , max_quantity = item[\'max_quantity\']\n
#                                                      )\n
 \n
# create budget cells objects  \n
base_cat_list = [\'financial_section\',\'function\']\n
for key in clean_budget_cells.keys():\n
      new_budget_cell = destination_obj.newContent( portal_type  = budget_cell_type\n
                                                  , max_quantity = clean_budget_cells[key][\'max_quantity\']\n
                                                  , membership_criterion_base_category = base_cat_list\n
                                                  )\n
\n
    \n
# return to the feature module\n
return context.REQUEST.RESPONSE.redirect(context.absolute_url() + \'?portal_status_message=Budget+Cells+added.\')\n
</string> </value>

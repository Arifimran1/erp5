        <item>
            <key> <string>_body</string> </key>
            <value> <string encoding="cdata"><![CDATA[

budget_list = context.getPortalObject().budget_module.objectValues()\n
budget_cell=[(\'\', \'\')]\n
for o in budget_list:\n
    if o.getPortalType() == \'Budget\':\n
    \n
         if date >= o.getStartDate() and date <= o.getStopDate():             \n
             lines = o.objectValues()\n
             for l in lines:\n
                cells = l.objectValues()\n
                if l.getGroup() == group:\n
                    for c in cells:\n
                           item=[]                           \n
                           title = c.getTitle()\n
                           item.append(title) \n
                           item.append(c.getRelativeUrl())\n
                           budget_cell.append(item) \n
\n
return budget_cell\n


]]></string> </value>

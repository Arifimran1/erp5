        <item>
            <key> <string>_body</string> </key>
            <value> <string encoding="cdata"><![CDATA[

from Products.Formulator.Errors import ValidationError, FormValidationError\n
\n
request=context.REQUEST\n
\n
try:\n
  for line in fast_input:\n
    line_items = line.split()\n
    if len(line_items) > 1:\n
      # Product and quantity\n
      line_product = \' \'.join(line_items[0:-1])\n
      line_quantity = line_items[-1]\n
    else:\n
      # Product \n
      line_product = line\n
    new_id = str(context.generateNewId())\n
    context.portal_types.constructContent(type_name="Inventory Line",\n
                                                            container=context,\n
                                                            id=new_id)\n
    resource_list = context.portal_catalog(title=line_product, portal_type=\'Product\', Title=line_product)\n
    if len(resource_list) > 0:\n
      resource_value = resource_list[0].getObject()\n
      if resource_value is not None:\n
        context[new_id].edit(inventory=line_quantity ,\n
                                     description=line_product ,\n
                                     resource_value = resource_value)\n
    else:\n
      context[new_id].edit( inventory=line_quantity ,\n
                                     description=line_product ,\n
                                                           )\n
except FormValidationError, validation_errors:\n
  redirect_url = \'%s/view?%s\' % ( context.absolute_url()\n
                                  , \'portal_status_message=Failed+Fast+Input.\'\n
                                  )\n
else:\n
  redirect_url = \'%s/view?%s\' % ( context.absolute_url()\n
                                  , \'portal_status_message=Data+Updated.\'\n
                                  )\n
\n
request[ \'RESPONSE\' ].redirect( redirect_url )\n


]]></string> </value>

        <item>
            <key> <string>_body</string> </key>
            <value> <string encoding="cdata"><![CDATA[

packing_list_list = context.getCausalityValueList(portal_type=[\'Sale Packing List\',\'Sales Packing List\'])\n
\n
if len(packing_list_list) > 0:\n
  packing_list = context.getCausalityValueList(portal_type=[\'Sale Packing List\',\'Sales Packing List\'])[0]\n
  order = packing_list.getCausalityValueList()[0]\n
  from DateTime import DateTime\n
  due_date = packing_list.getTargetStopDate( DateTime() )\n
  pat = None #order.getPaymentAdditionalTerm()\n
else:\n
  due_date = context.getStartDate()\n
  pat = None\n
\n
due_date += context.getPaymentTerm(30)\n
peom = context.getPaymentEndOfMonth()\n
\n
if peom:\n
  i = 0\n
  month = due_date.month()\n
  while (month == (due_date + i).month()):\n
    i += 1\n
  due_date = (due_date + i - 1)\n
\n
  if pat != None:\n
    due_date += pat\n
\n
else:\n
  if pat != None:\n
    i = 0\n
    month = due_date.month()\n
    while (month == (due_date + i).month()):\n
      i -= 1\n
    due_date = (due_date + i + pat)\n
\n
return due_date\n


]]></string> </value>

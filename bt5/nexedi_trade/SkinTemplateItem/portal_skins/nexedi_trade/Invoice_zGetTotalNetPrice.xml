        <item>
            <key> <string>_body</string> </key>
            <value> <string encoding="cdata"><![CDATA[

# price = context.getTotalPrice()\n
price = context.Invoice_zGetTotal()[0].total_price\n
\n
discount_list_tmp = context.contentValues(filter={\'portal_type\':\'Remise\'})\n
discount_list = filter(lambda x: x not in [None,0] ,discount_list_tmp)\n
\n
if len(discount_list) > 1:\n
  discount_list.sort(lambda x,y: cmp(x.getIntIndex(),y.getIntIndex()))\n
\n
for discount_line in discount_list:\n
  if discount_line.getImmediateDiscount():\n
    price *= (1 - discount_line.getDiscountRatio())\n
\n
return price\n


]]></string> </value>

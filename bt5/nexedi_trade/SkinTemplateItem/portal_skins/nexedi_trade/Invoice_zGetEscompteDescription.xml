        <item>
            <key> <string>_body</string> </key>
            <value> <string>tmp_list = context.contentValues(filter={\'portal_type\':\'Remise\', \'discount_type_title\':\'Escompte\'})\n
\n
escompte_list = filter(lambda item: item.getDiscountType() == \'Escompte\', tmp_list)\n
\n
if escompte_list != []:\n
  escompte_object = escompte_list[0]\n
  if escompte_object.getDescription() != None: \n
    escompte_description = string.replace(escompte_object.getDescription(),\'%\',\'%%\')[:45]\n
  elif escompte_object.getDiscountRatio() != None:\n
    escompte_description = \'%.2f\' % (escompte_object.getDiscountRatio() * 100) + \'%% sous 10 jours\'\n
  else:\n
    escompte_description = \'2%% sous 10 jours\'\n
else:\n
  escompte_description = \'2%% sous 10 jours\'\n
\n
return escompte_description\n
</string> </value>

            <key> <string>_body</string> </key>
            <value> <string encoding="cdata"><![CDATA[

# Script used to limit the values of a list found in a category\n
\n
\n
# Values to delete from returned list\n
# For example : { \'network_interface\': [ None, (\'ppp\',) ], ... }\n
# In this case, if the script is called to return the category \'network_interface\',\n
# it will display all of the category values if interface_number = 0,\n
# and all of the category values except the \'ppp\' subcategory if interface_number = 1\n
\n
forbidden = { \'network_interface\':[ None,\n
                                    None,\n
                                    (\'ppp\',),\n
                                    (\'ppp\',\'wlan\') ],\n
              \'network_type\':     [ None,\n
                                    None,\n
                                    (\'ppp\',),\n
                                    (\'ppp\', \'wireless\') ]\n
             }\n
\n
\n
return_value = [ ("","") ]\n
try:\n
  category = context.portal_categories[category_name]\n
except:\n
  return return_value\n
\n
\n
\n
# Get the values of forbidden categories in this case\n
forbidden_here = []\n
try:\n
  forb = forbidden[category_name][int(interface_number)]\n
  if forb:\n
    forbidden_here = forb\n
except:\n
  pass\n
\n
\n
# Get forbidden categories\n
forbidden_categories = []\n
for forb in forbidden_here:\n
  try:\n
    cat = category[forb].getCategoryChildItemList()\n
    if len(cat) > 1:\n
      forbidden_categories.extend(cat[1:])\n
  except:\n
    pass\n
\n
\n
\n
# Get all categories\n
categories = category.getCategoryChildItemList()\n
\n
\n
\n
for couple in categories:\n
  # Filter the content\n
  to_add = 1\n
  for forb in forbidden_categories:\n
    if forb[0] == couple[0]:\n
      to_add = 0\n
      break\n
  if to_add:\n
    for forb in forbidden_here:\n
      if forb == couple[0]:\n
        to_add = 0\n
        break\n
\n
  # Correct the displayed value by discarding the path\n
  if to_add:\n
    splitted = [[],[]]\n
    value = ["",""]\n
    splitted = couple[0].split("/")\n
    value = splitted[len(splitted)-1]\n
    splitted = couple[1].split("/")\n
    value2 = splitted[len(splitted)-1]\n
\n
    return_value.append( (value,value2) )\n
\n
return return_value\n


]]></string> </value>

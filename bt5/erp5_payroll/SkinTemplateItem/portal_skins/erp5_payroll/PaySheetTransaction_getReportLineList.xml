        <item>
            <key> <string>_body</string> </key>
            <value> <string>report_items = context.PaySheetTransaction_zGetDetailedTotal()\n
\n
report_lines = []\n
\n
# scan every sql report item\n
for item in report_items:\n
    line =  { \'title\'               : None\n
            , \'employer_totalbase\'  : None\n
            , \'employer_rate\'       : None\n
            , \'employer_total\'      : None\n
            , \'employee_totalbase\'    : None\n
            , \'employee_rate\'         : None\n
            , \'employee_total\'        : None\n
            , \'total\'               : None\n
            }\n
    # sort by employer/salary share\n
    if item[\'variation_text\'].find(\'employee_share\') != -1:\n
        line[\'title\']               = item[\'parent_title\']\n
        line[\'employee_totalbase\']    = item[\'base\']\n
        line[\'employee_rate\']         = item[\'rate\']\n
        line[\'employee_total\']        = item[\'total_price\']\n
    if item[\'variation_text\'].find(\'employer_share\') != -1:\n
        line[\'title\']               = item[\'parent_title\']\n
        line[\'employer_totalbase\']  = item[\'base\']\n
        line[\'employer_rate\']       = item[\'rate\']\n
        line[\'employer_total\']      = item[\'total_price\']\n
    report_lines.append(line)\n
\n
# scan every line and group them\n
\n
#here.portal\n
\n
# first grouping:\n
\n
\n
return report_lines\n
</string> </value>

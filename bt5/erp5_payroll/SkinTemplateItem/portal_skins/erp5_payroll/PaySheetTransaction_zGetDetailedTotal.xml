        <item>
            <key> <string>src</string> </key>
            <value> <string encoding="cdata"><![CDATA[

SELECT\n
\n
  SUM(MovementCell.total_price) as total\n
\n
FROM\n
\n
  catalog PaySheet,\n
  catalog Line,\n
  catalog Cell,\n
  movement MovementCell,\n
  category CategoryCell,\n
  catalog TaxCategory,\n
  category CategoryPaySheet\n
\n
WHERE\n
  \n
      PaySheet.uid = Line.parent_uid\n
  AND Cell.parent_uid = Line.uid\n
  AND MovementCell.uid = Cell.uid\n
  AND CategoryCell.uid = Cell.uid \n
  AND TaxCategory.uid = CategoryCell.category_uid \n
  AND TaxCategory.id = <dtml-sqlvar tax_category type="string">\n
  AND PaySheet.uid = CategoryPaySheet.uid\n
  AND CategoryPaySheet.base_category_uid=\'-138283359\' \n
  AND CategoryPaySheet.category_uid=\'-1774242116\' \n
  AND MovementCell.start_date>=<dtml-sqlvar start_date type="string">\n
  AND MovementCell.stop_date<=<dtml-sqlvar stop_date type="string">\n
  GROUP BY CategoryPaySheet.category_uid\n


]]></string> </value>

  if list_item[\'selected\'] == 1:\n
     checkbook_item = context.portal_catalog(portal_type = list_item[\'portaltype\'],id = list_item[\'checkbook\'])\n
#context.portal_simulation.getTrackingList(at_date= default_reference_date, node = default_node, section = default_section_node):\n
     curr_object = checkbook_item[0].getObject()\n
\n
# Lire la variation de la resource "type de checkbook" qui est un sous object du Type de Checkbook\n
     resource_object = [x.getObject().getRelativeUrl() for x in curr_object.getResourceValue().contentValues() \\\n
     if curr_object.getCheckbookType() == x.getObject().getId()]\n
     \n
     #raise \'MoulError\',repr(resource_object)\n
     new_checkD = context.newContent(portal_type=line_PortalType)        \n
     #new_checkD.setQuantity(checkbook_item.getQuantity())\n
     new_checkD.setResource(curr_object.getResource())\n
     new_checkD.setCheckbookType(resource_object)\n
     new_checkD.setCheckNumberRangeStop(curr_object.getCheckNumberRangeStop())\n
     new_checkD.setCheckNumberRangeStart(curr_object.getCheckNumberRangeStart())\n
     new_checkD.setResource(curr_object.getResource())\n
     new_checkD.setQuantity(list_item[\'quantity\'])\n
     new_checkD.setAggregateValueList(curr_object)\n
     \n
     counter_line += 1\n
#return (default_section_node,default_node,default_reference_date)\n
\n
\n
request  = context.REQUEST\n
redirect_url = \'%s/view?%s\' % ( context.absolute_url()\n
                                , \'portal_status_message=\'+str(counter_line)+ \' + ligne(s)+cr\xc3\xa9\xc3\xa9e(s)...\'\n
                                )\n
request[ \'RESPONSE\' ].redirect( redirect_url )\n

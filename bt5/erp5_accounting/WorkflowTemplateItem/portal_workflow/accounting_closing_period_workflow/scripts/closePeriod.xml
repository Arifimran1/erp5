        <item>
            <key> <string>_body</string> </key>
            <value> <string encoding="cdata"><![CDATA[

from Products.DCWorkflow.DCWorkflow import ValidationFailed\n
\n
closing_period = state_change.object\n
\n
start_date = closing_period.getStartDate()\n
stop_date = closing_period.getStopDate()\n
\n
if stop_date < start_date :\n
  raise ValidationFailed, "Action Impossible: stop_date is before start_date"\n
\n
for cpd in context.portal_catalog(\n
         portal_type="Accounting Period Delimiter", \n
         simulation_state="delivered") : \n
   cpd = cpd.getObject()\n
   if cpd.getStartDate() < start_date <  cpd.getStopDate():\n
     raise ValidationFailed, "Action Impossible: start_date is in a closed period"\n
   if cpd.getStartDate() < stop_date <  cpd.getStopDate():\n
     raise ValidationFailed, "Action Impossible: stop_date is in a closed period"\n
\n
# TODO: deliver every transaction in the period (in accounting_workflow)\n


]]></string> </value>

            <key> <string>text_content</string> </key>
            <value> <string encoding="cdata"><![CDATA[

/*global window, rJS, RSVP, document */\n
/*jslint indent: 2, maxerr: 3 */\n
(function (window, rJS, RSVP) {\n
  "use strict";\n
\n
  rJS(window)\n
    .ready(function (gadget) {\n
      return gadget.getElement()\n
        .push(function (element) {\n
          gadget.props = {};\n
          gadget.props.element = element;\n
        });\n
    })\n
    .declareAcquiredMethod("notifyInvalid", "notifyInvalid")\n
    .declareAcquiredMethod("notifyValid", "notifyValid")\n
    .declareMethod(\'render\', function (options) {\n
      var radio_group = this.props.element.querySelector(".radiogroup"),\n
        i,\n
        input,\n
        label,\n
        div,\n
        field_json = options.field_json,\n
        value = field_json.default || "",\n
        editable = field_json.editable,\n
        items = field_json.items;\n
      //the first item will always be selected if no initial default value is supplied.\n
      if (value === "" && field_json.select_first_item) {\n
        value = items[0][1];\n
      }\n
      this.props.field_json = field_json;\n
      for (i = 0; i < items.length; i += 1) {\n
        div = document.createElement("div");\n
        div.setAttribute("class", "ui-field-contain");\n
        input = document.createElement("input");\n
        input.setAttribute("class", "ui-btn");\n
        input.setAttribute("type", "radio");\n
        input.setAttribute("name", field_json.key);\n
        input.setAttribute("value", items[i][1]);\n
        if (items[i][1] === value) {\n
          input.setAttribute("checked", true);\n
        }\n
        if (editable === 0) {\n
          input.setAttribute("class", "ui-btn ui-state-disabled");\n
        }\n
        label = document.createElement("label");\n
        label.setAttribute(\'for\', items[i][0]);\n
        label.textContent = items[i][0];\n
        label.setAttribute(\'data-i18n\', items[i][0]);\n
        \n
        div.appendChild(label);\n
        div.appendChild(input);\n
        radio_group.appendChild(div);\n
      }\n
      radio_group.setAttribute("data-type", field_json.orientation);\n
    })\n
    .declareMethod(\'checkValidity\', function () {\n
      var gadget = this,\n
        json_field = gadget.props.field_json;\n
      return new RSVP.Queue()\n
        .push(function () {\n
          return gadget.notifyValid();\n
        })\n
        .push(function () {\n
          return gadget.getContent();\n
        })\n
        .push(function (result) {\n
          if (json_field.required && !result.hasOwnProperty(json_field.key)) {\n
            return gadget.notifyInvalid("This field is required");\n
          }\n
          return true;\n
        });\n
    })\n
    .declareMethod(\'getContent\', function () {\n
      var inputs = this.props.element.querySelectorAll("input"),\n
        i,\n
        result = {};\n
      for (i = 0; i < inputs.length; i += 1) {\n
        if (inputs[i].checked === true) {\n
          result[this.props.field_json.key] = inputs[i].value;\n
          break;\n
        }\n
      }\n
      return result;\n
    });\n
}(window, rJS, RSVP));

]]></string> </value>

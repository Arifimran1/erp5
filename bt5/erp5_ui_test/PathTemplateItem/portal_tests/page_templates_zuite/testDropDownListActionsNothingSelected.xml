</thead><tbody>\n
<tal:block metal:use-macro="here/PTZuite_CommonTemplate/macros/init" />\n
<tr>\n
  <td>open</td>\n
  <td tal:content="string: ${here/portal_url}/foo_module/1/view">/erp5/foo_module/1/view</td>\n
  <td></td>\n
</tr>\n
<!-- when clicking on the action button of a drop down when nothing is selected\n
shouldn\'t do anything. -->\n
<tal:block tal:repeat="button_title python:[\'Select Favorite\',\n
                                            \'Select Module\',\n
                                            \'Select Language\',\n
                                            \'Jump\',\n
                                            \'Action\']">\n
  <tr>\n
    <td>click</td>\n
    <td tal:content="string://button[@title=\'${button_title}\']"/>\n
    <td></td>\n
  </tr>\n

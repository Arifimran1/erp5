        <item>
            <key> <string>_body</string> </key>
            <value> <string>from Products.DCWorkflow.DCWorkflow import ValidationFailed\n
from Products.ERP5Type.Message import Message\n
\n
transaction = state_change[\'object\']\n
\n
# Guess counter from user assignment\n
user_id = transaction.portal_membership.getAuthenticatedMember().getUserName()\n
user_site_list = context.Baobab_getUserAssignedSiteList(user_id=user_id)\n
\n
default_currency = "currency_module/%s" %transaction.Baobab_getPortalReferenceCurrencyID()\n
currency = transaction.getResource()\n
counter = transaction.getSource()\n
\n
baobab_counter = None\n
for site in user_site_list:\n
  site_value = context.portal_categories.getCategoryValue(site)\n
  if site_value.getVaultType().endswith(\'guichet\') and counter in site:\n
    if currency == default_currency:\n
      baobab_source = site + \'/encaisse_des_billets_et_monnaies/sortante\'\n
    else:\n
      vault_title = (transaction.getResourceTitle().lower()).replace(\' \', \'_\')\n
      baobab_source = site + \'/encaisse_des_devises/%s/sortante\' %vault_type\n
    break\n
\n
\n
# check balance\n
price = transaction.getSourceTotalAssetPrice()\n
resource = transaction.CashDelivery_checkCounterInventory(source=baobab_source,\n
                                                          portal_type=\'Outgoing Incident Line\',)\n
if resource == 3:\n
  msg = Message(domain="ui", message="No banknote or coin defined.")\n
  raise ValidationFailed, (msg,)\n
elif resource == 2:\n
  msg = Message(domain="ui", message="No resource defined.")\n
  raise ValidationFailed, (msg,)\n
elif resource == 1:\n
  msg = Message(domain="ui", message="Insufficient Balance in counter.")\n
  raise ValidationFailed, (msg,)\n
</string> </value>

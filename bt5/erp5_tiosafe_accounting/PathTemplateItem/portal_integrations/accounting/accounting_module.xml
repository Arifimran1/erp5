        <item>
            <key> <string>src</string> </key>
            <value> <string encoding="cdata"><![CDATA[

SELECT\n
  DISTINCT(journal_code) AS type,\n
  transaction_reference AS reference,\n
  IF (journal_code in (\'PI\', \'PC\'), CONCAT(\'Organisation \', third_party), \'Organisation MyFakeGidOrg\') AS source_ownership,\n
  IF (journal_code in (\'PI\', \'PC\'), \'Organisation MyFakeGidOrg\', CONCAT(\'Organisation \', third_party)) AS destination_ownership,\n
  date AS start_date,\n
  date AS stop_date,\n
  CONCAT(\'Tax Code/\', tax_code, \'\\nJournal/\', journal_code) AS category,\n
  CONCAT(\'<dtml-var getPath>/accounting_module/\', transaction_reference, \' \', journal_code, \' \', date) AS path,\n
  CONCAT(\'Accounting \', transaction_reference, \' \', journal_code, \' \', date) AS gid\n
FROM\n
  NOMACTX\n
<dtml-if id>\n
WHERE\n
  CONCAT(transaction_reference, \' \', journal_code, \' \', date) = <dtml-sqlvar type="string" expr="id">\n
</dtml-if>\n
GROUP BY\n
  transaction_reference,\n
  journal_code,\n
  date\n
ORDER BY\n
  transaction_reference ASC,\n
  journal_code ASC,\n
  date ASC\n


]]></string> </value>
